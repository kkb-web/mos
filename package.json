{
  "name": "cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cli-run --mode dev --path scripts/start.js",
    "start": "cli-run --mode dev --path scripts/start.js",
    "test": "cli-run --mode test --path scripts/build.js",
    "pre": "cli-run --mode pre --path scripts/build.js",
    "prod": "cli-run --mode prod --path scripts/build.js",
    "tag": "node ./node_modules/easy-tag",
    "build": "node scripts/build.js"
  },
  "dependencies": {
    "@rematch/core": "^1.1.0",
    "@rematch/loading": "^1.1.3",
    "@sentry/browser": "^5.5.0",
    "@tinymce/tinymce-react": "^2.2.5",
    "ant-design-pro": "1.4.4",
    "antd": "3.9.2",
    "autoprefixer": "7.1.2",
    "axios": "^0.17.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cli-run": "^1.0.2",
    "crypto-js": "^3.1.9-1",
    "css-loader": "0.28.4",
    "dingtalk-jsapi": "^2.6.44",
    "dotenv": "4.0.0",
    "easy-tag": "^1.0.1",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.0",
    "echarts-gl": "^1.0.0-beta.5",
    "echarts-liquidfill": "^1.1.1",
    "echarts-wordcloud": "^1.1.3",
    "eslint": "4.19.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.19.1",
    "mongodb": "^2.2.33",
    "numeral": "^2.0.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "qrcode.react": "^0.8.0",
    "react": "^16.0.0",
    "react-core-image-upload": "^2.2.3",
    "react-countup": "^2.3.0",
    "react-cropper": "^1.0.1",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-native-qrcode": "^0.2.6",
    "react-redux": "^7.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sockjs-client": "^1.3.0",
    "stompjs": "^2.3.3",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "webpackbar": "^3.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "eqeqeq": ["off"]
    }
  },
  "proxy": "https://api.weixin.qq.com"
}
